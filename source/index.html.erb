---
title: Le Wagon Middleman boilerplate
---
<!-- Your banner code -->
<div class="banner" style="background-image: linear-gradient(-225deg, rgba(0,101,168,0.6) 0%, rgba(0,36,61,0.6) 50%), url('<%= image_path "images/background.jpg" %>');">
  <div class="banner-content">
    <h1>WELCOME TO THE JUNGLE</h1>
    <p>Rent my jam with style</p>

    <form action="#" class="form-inline">
      <input type="text" class="form-control input-lg" placeholder="Where">
      <input type="text" class="form-control input-lg" placeholder="Check In">
      <input type="text" class="form-control input-lg" placeholder="Check Out">

      <select name="#" id="" class="form-control input-lg">
        <option value="1">1 guest</option>
        <option value="2">2 guests</option>
        <option value="3">3 guests</option>
        <option value="4">4 guests</option>
      </select>

      <a class="btn btn-primary btn-lg">Search</a>


    </form>
  </div>
</div>

<!-- Your features code -->
      <div class="text-center padded features">

        <div class="container">
          <div class="row">
            <div class="col-xs-12 col-sm-6 col-md-3">
              <%= image_tag "images/globe.png" %>
              <h2>Discover the world</h2>
              <p>Raccoon is your home.</p>
            </div>

            <div class="col-xs-12 col-sm-6 col-md-3">
              <%= image_tag "images/dollar.png" %>
              <h2>Pay Online</h2>
              <p>No cash issues anymore.</p>
            </div>

            <div class="col-xs-12 col-sm-6 col-md-3">
              <%= image_tag "images/beer.png" %>
              <h2>Meet people</h2>
              <p>Discover new culture and lifestyle.</p>
            </div>

            <div class="col-xs-12 col-sm-6 col-md-3">
              <%= image_tag "images/heart.png" %>
              <h2>Share and love</h2>
              <p>Join a community you will love.</p>
            </div>
          </div>
        </div>
      </div>

      <!-- Your cards code -->
      <div class="wrapper-grey padded">
        <h2 class="text-center">Your next trip</h2>
        <div class="container">
          <div class="row">
            <div class="col-xs-12 col-sm-4">
              <!-- insert <div class="card"> -->
              <div class="card" style="background: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.2)), url('<%= image_path "images/hbs.jpg" %>');">
                <div class="card-category">Boston</div>
                <div class="card-description">
                  <h2>Remi's home</h2>
                  <p>HBS bitchessss!</p>
                </div>
                <%= image_tag "images/remi.jpeg", class: "card-user avatar avatar-large" %>
                <a href="#" class="card-link"></a>
              </div>
            </div>
            <div class="col-xs-12 col-sm-4">
              <!-- insert <div class="card"> -->
              <div class="card" style="background: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.2)), url('<%= image_path "images/card-pb.jpg" %>');">
                <div class="card-category">Tokyo</div>
                <div class="card-description">
                  <h2>Taka's home</h2>
                  <p>Peanut butter hell</p>
                </div>
                <%= image_tag "images/taka.jpeg", class: "card-user avatar avatar-large" %>
                <a href="#" class="link"></a>
              </div>
            </div>
            <div class="col-xs-12 col-sm-4">
              <!-- insert <div class="card"> -->
              <div class="card" style="background: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.2)), url('<%= image_path "images/money.jpg" %>');">
                <div class="card-category">London</div>
                <div class="card-description">
                  <h2>Archie's home</h2>
                  <p>Get me dat money ye boi</p>
                </div>
                <%= image_tag "images/archie.jpeg", class: "card-user avatar avatar-large" %>
                <a href="#" class="link"></a>
              </div>
            </div>
          </div>
        </div>
      </div>


      <!-- Your newsletter code -->
      <div class="text-center padded id="newsletter">
        <h2>Get news from us</h2>
        <p>Leave us your mail and get news from us</p>
        <form action="#" class="form-inline">
          <input type="email" placeholder="steve@gmail.com" class="form-control">
          <input type="submit" value="subscribe" class="btn btn-primary">
        </form>
      </div>

      <!-- Your map code -->
      <div id="map" style="width: 100%; height: 400px;"></div>

      <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js"></script>

      <script type="text/javascript">
        var myLatlng = new google.maps.LatLng(48.8655909, 2.378983);

        var myOptions = {
          zoom: 16,
          center: myLatlng,
          scrollwheel: false,
          mapTypeId: google.maps.MapTypeId.ROADMAP,
          styles: [{"featureType":"administrative","elementType":"all","stylers":[{"saturation":"-100"}]},{"featureType":"administrative.province","elementType":"all","stylers":[{"visibility":"off"}]},{"featureType":"landscape","elementType":"all","stylers":[{"saturation":-100},{"lightness":65},{"visibility":"on"}]},{"featureType":"poi","elementType":"all","stylers":[{"saturation":-100},{"lightness":"50"},{"visibility":"simplified"}]},{"featureType":"road","elementType":"all","stylers":[{"saturation":"-100"}]},{"featureType":"road.highway","elementType":"all","stylers":[{"visibility":"simplified"}]},{"featureType":"road.arterial","elementType":"all","stylers":[{"lightness":"30"}]},{"featureType":"road.local","elementType":"all","stylers":[{"lightness":"40"}]},{"featureType":"transit","elementType":"all","stylers":[{"saturation":-100},{"visibility":"simplified"}]},{"featureType":"water","elementType":"geometry","stylers":[{"hue":"#ffff00"},{"lightness":-25},{"saturation":-97}]},{"featureType":"water","elementType":"labels","stylers":[{"lightness":-25},{"saturation":-100}]}]
        };

        var map = new google.maps.Map(document.getElementById('map'), myOptions);
        var marker = new google.maps.Marker({
          position: myLatlng,
          map: map,
          title: "You are here!"
        });
      </script>




